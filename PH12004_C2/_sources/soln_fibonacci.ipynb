   "source": [
    "fibonacci = np.zeros(20)\n",
    "for n in range(20):\n",
    "    if n < 2:\n",
    "        fibonacci[n] = 1\n",
    "    else:\n",
    "        fibonacci[n] = fibonacci[n-2] + fibonacci[n-1]\n",
    "print(fibonacci)"
